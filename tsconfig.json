{
   "compilerOptions": {
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "ESNext",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noEmit": true,
      "allowImportingTsExtensions": true,
   },
   "ts-node": {
      "esm": true
  }
}